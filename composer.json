{
	"name": "haykay/mvc-boilerplate",
	"description": "A simple PHP MVC Boilerplate",
	"keywords": [
		"framework",
		"MVC",
		"Boilerplate"
	],
	"license": "MIT",
	"type": "project",
	"require": {
		"php": ">=8.2",
		"twbs/bootstrap": "@stable",
		"vlucas/phpdotenv": "@stable"
	},
	"autoload": {
		"classmap": [
			"./"
		]
	},
	"scripts": {
        "post-update-cmd": [
			"php scripts/cpdir.php vendor/twbs/bootstrap/dist public/vendor/bootstrap"
        ]
    }
}